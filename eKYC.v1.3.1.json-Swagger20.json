{
  "openapi": "3.0.0",
  "info": {
    "title": "eKYC",
    "version": "1.3",
    "description": "eKYC APIs can be used by any external system to communicate with the eKYC platform, primarily for KYC Upload and Query KYC information. eKYC system also provides the APIs for National ID and Dealer Check",
    "contact": {
      "name": "Tayana Software Solutions Pvt. Ltd."
    }
  },
  "tags": [
    {
      "name": "KYC"
    }
  ],
  "servers": [
    {
      "url": "http://localhost:3000/ekyc/v1"
    }
  ],
  "paths": {
    "/dealer/{msisdn}": {
      "parameters": [
        {
          "schema": {
            "type": "string",
            "pattern": "[0-9]{6,11}",
            "minLength": 6,
            "maxLength": 11
          },
          "name": "msisdn",
          "in": "path",
          "description": "Dealer MSISDN",
          "required": true
        }
      ],
      "get": {
        "summary": "Query Dealer Info",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/QueryDealerResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "operationId": "get-dealer-msisdn",
        "description": "Query Dealer API is invoked with the Dealer MSISDN, and eKYC System checks internally whether Any Dealer Exists in system with given MSISDN. If Dealer exists the eKYC system returns the Dealer Information.",
        "parameters": [
          {
            "schema": {
              "type": "string",
              "minLength": 0,
              "maxLength": 30
            },
            "in": "query",
            "name": "refId",
            "description": "Application Reference Id, if Any"
          }
        ]
      }
    },
    "/dealer/{msisdn}/changepin": {
      "parameters": [
        {
          "schema": {
            "type": "string"
          },
          "name": "msisdn",
          "in": "path",
          "description": "Dealer MSISDN",
          "required": true
        }
      ],
      "put": {
        "summary": "ChangePin",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "operationId": "put-dealer-msisdn-changepin",
        "description": "Dealer Change-PIN API - This API is used to Change PIN for Dealer",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChangePinRequest"
              }
            }
          },
          "description": "Old PIN and New PIN should be provided in the request content along with the Application Reference Id"
        }
      }
    },
    "/nid": {
      "post": {
        "summary": "NID Status Query",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NIDStatusQueryResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "operationId": "post-nid",
        "description": "This API can be used to Check the given NID for total number of KYCs registered against given NID, In response to this API call eKYC system returns whether NID is registered with eKYC system or not. API returns the number of SIMs registered against the NID, if NID is registered.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/NIDStatusRequest"
              }
            }
          },
          "description": ""
        },
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "dealer",
            "description": "MSISDN of Dealer who is initiating the request"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "pin",
            "description": "Dealer PIN"
          }
        ]
      }
    },
    "/id": {
      "post": {
        "summary": "ID Status Query",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IDStatusQueryResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "operationId": "post-id",
        "description": "This API can be used to Check the total number of KYCs registered against given ID, In response to this API call eKYC system returns whether ID is registered with eKYC system or not. API returns the number of SIMs registered against the ID, if ID is registered.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/IDStatusRequest"
              }
            }
          },
          "description": ""
        },
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "dealer",
            "description": "MSISDN of Dealer who is initiating the request"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "pin",
            "description": "Dealer PIN"
          }
        ]
      }
    },
    "/kyc/{searchType}/{searchNumber}/status": {
      "parameters": [
        {
          "schema": {
            "type": "string",
            "enum": [
              "sim",
              "imsi",
              "mobile",
              "tid"
            ]
          },
          "name": "searchType",
          "in": "path",
          "required": true,
          "description": "KYC Status Query InfoType to be searched"
        },
        {
          "schema": {
            "type": "string",
            "pattern": "[0-9]{20}",
            "minLength": 3,
            "maxLength": 20
          },
          "name": "searchNumber",
          "in": "path",
          "required": true,
          "description": "Interpretation of SearchNumbr depends on the \"searchType\" Field"
        }
      ],
      "get": {
        "summary": "KYC Status Query",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/KYCStatusResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "operationId": "get-kyc-status-searchType-searchNumber",
        "description": "This API can be used to query the KYC status based on ServiceNumber, SIM No, IMSI or Transaction Id",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "dealer",
            "description": "MSISDN of Dealer who is initiating the request"
          }
        ]
      }
    },
    "/kyc/{searchType}/{searchNumber}": {
      "get": {
        "summary": "KYC Query",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/KYCQueryResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "operationId": "get-kyc",
        "description": "This API can be used to Retrieve the KYC Info based on ServiceNumber, SIM No, IMSI",
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "dealer",
            "description": "MSISDN of Dealer who is initiating the request"
          }
        ]
      },
      "parameters": [
        {
          "schema": {
            "type": "string",
            "enum": [
              "sim",
              "imsi",
              "mobile"
            ]
          },
          "name": "searchType",
          "in": "path",
          "description": "KYC Query InfoType to be searched",
          "required": true
        },
        {
          "schema": {
            "type": "string"
          },
          "name": "searchNumber",
          "in": "path",
          "required": true,
          "description": "Interpretation of SearchNumbr depends on the \"searchType\" Field"
        }
      ],
      "post": {
        "summary": "Create KYC Record/Upload KYC",
        "tags": [],
        "operationId": "post-kyc-searchType-searchNumber",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "description": "This API can be used to create the KYC Record on eKYC system and to upload the required KYC Documents.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/KYCRecord"
              }
            }
          },
          "description": "KYC Record to be created on eKYC system"
        },
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "dealer",
            "description": "MSISDN of Dealer who is initiating the request"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "pin",
            "description": "Dealer PIN"
          }
        ]
      },
      "put": {
        "summary": "Update KYC (Re-KYC)",
        "tags": [],
        "operationId": "put-kyc-searchType-searchNumber",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "description": "This API can be used to update the KYC Record on eKYC system (Re-KYC)",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/KYCRecord"
              }
            }
          },
          "description": "Re-KYC or KYC Update Information"
        },
        "parameters": [
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "dealer",
            "description": "MSISDN of Dealer who is initiating the request"
          },
          {
            "schema": {
              "type": "string"
            },
            "in": "header",
            "name": "pin",
            "description": "Dealer PIN"
          }
        ]
      }
    },
    "/dealer/{msisdn}/transactions": {
      "parameters": [
        {
          "schema": {
            "type": "string"
          },
          "name": "msisdn",
          "in": "path",
          "required": true
        }
      ],
      "get": {
        "summary": "Dealer Transactions History",
        "tags": [],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DealerTxnHistoryResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request"
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GenericResponse"
                }
              }
            }
          },
          "403": {
            "description": "Forbidden"
          },
          "404": {
            "description": "Not Found"
          },
          "500": {
            "description": "Internal Server Error"
          }
        },
        "operationId": "get-dealer-msisdn-history",
        "description": "This API can be used to retrieve the Dealer Transaction History from eKYC System",
        "parameters": [
          {
            "schema": {
              "type": "integer"
            },
            "in": "query",
            "name": "records",
            "description": "Maximum Number of Transaction Records to be retrieved in response"
          },
          {
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "in": "query",
            "name": "from",
            "description": "Dealer Transactions to be retrieved from given date"
          },
          {
            "schema": {
              "type": "string",
              "format": "date-time"
            },
            "in": "query",
            "name": "to",
            "description": "Dealer Transactions to be retrieved upto given date"
          },
          {
            "schema": {
              "enum": [
                "NID_QUERY",
                "KYC_STATUS",
                "KYC_QUERY",
                "KYC_CREATE",
                "KYC_UPDATE",
                "CHANGE_PIN"
              ],
              "type": "string"
            },
            "in": "query",
            "name": "txnType",
            "description": "Comma separated list of Transaction Types to be retrieved: NID_QUERY, KYC_STATUS, KYC_QUERY, KYC_CREATE, KYC_UPDATE, CHANGE_PIN"
          }
        ]
      }
    }
  },
  "components": {
    "schemas": {
      "Result": {
        "title": "Result",
        "type": "object",
        "x-tags": [
          "result"
        ],
        "x-examples": {
          "example-1": {}
        },
        "description": "",
        "properties": {
          "code": {
            "type": "integer",
            "description": "The unique response code that indicates the response status of the request. See the Response codes for more detailed information"
          },
          "message": {
            "type": "string",
            "description": "Response description of the response code",
            "minLength": 0,
            "maxLength": 100
          },
          "requestId": {
            "$ref": "#/components/schemas/ApplicationRequestId"
          },
          "referenceId": {
            "type": "string",
            "description": "eKYC System Internal Reference Identifier",
            "minLength": 0,
            "maxLength": 30
          },
          "error": {
            "type": "array",
            "description": "List of Error Message(s)",
            "uniqueItems": true,
            "items": {
              "type": "string",
              "minLength": 0,
              "maxLength": 150
            }
          }
        },
        "required": [
          "code"
        ]
      },
      "DealerInfo": {
        "title": "DealerInfo",
        "type": "object",
        "x-tags": [
          "Dealer"
        ],
        "description": "eKYC Dealer's Information. The information includes Dealer MSISDN, Name, email, Creation & Expiry Date along with Status",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64",
            "description": "Dealer ID"
          },
          "msisdn": {
            "type": "string",
            "description": "Dealer MSISDN",
            "pattern": "^([\\+]?)([0-9]){6,14}"
          },
          "name": {
            "type": "string",
            "maxLength": 50,
            "minLength": 1,
            "description": "Dealer Name"
          },
          "businessName": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50,
            "description": "Dealer's Business Name"
          },
          "email": {
            "type": "string",
            "format": "email",
            "description": "Dealer Email-Id",
            "maxLength": 50,
            "minLength": 0
          },
          "address": {
            "type": "string",
            "maxLength": 100,
            "minLength": 0
          },
          "creationDate": {
            "type": "string",
            "format": "date-time"
          },
          "expiryDate": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "string"
          }
        },
        "required": [
          "id",
          "msisdn",
          "status"
        ]
      },
      "GenericResponse": {
        "title": "GenericResponse",
        "type": "object",
        "properties": {
          "result": {
            "$ref": "#/components/schemas/Result"
          }
        },
        "description": "Generic Response Object to communicate only result without any additional operation related data/information"
      },
      "ApplicationRequestId": {
        "type": "string",
        "title": "ApplicationRequestId",
        "description": "Application Request ID",
        "maxLength": 15,
        "pattern": "(^[A-Za-z0-9])([A-Za-z0-9-_]+)"
      },
      "NIDInfo": {
        "title": "NIDInfo",
        "type": "object",
        "description": "KYC counts associated with National ID",
        "properties": {
          "nid": {
            "type": "string",
            "description": "National ID",
            "minLength": 1,
            "maxLength": 30
          },
          "status": {
            "type": "string",
            "enum": [
              "FOUND",
              "NOT_FOUND"
            ],
            "description": "Whether given NID is found in eKYC Subscriber database or not"
          },
          "registeredKYC": {
            "type": "integer",
            "format": "int32",
            "description": "Total Registered KYC(s) with given NID"
          },
          "activeKYC": {
            "type": "integer",
            "format": "int32",
            "description": "Total Active KYC(s) with given NID"
          },
          "allowedKYC": {
            "type": "integer",
            "format": "int32",
            "description": "Remaining Allowed KYC(s) for given NID"
          }
        },
        "required": [
          "nid",
          "status",
          "allowedKYC"
        ]
      },
      "IDInfo": {
        "title": "IDInfo",
        "type": "object",
        "description": "KYC counts associated with requested ID",
        "properties": {
          "type": {
            "type": "string",
            "enum": [
              "NID",
              "PASSPORT",
              "DL",
              "CORP_REG",
              "PHOTO",
              "OTHERS"
            ]
          },
          "id": {
            "type": "string",
            "description": "Identity Number",
            "minLength": 0,
            "maxLength": 30
          },
          "status": {
            "type": "string",
            "enum": [
              "FOUND",
              "NOT_FOUND"
            ],
            "description": "Whether given ID is found in eKYC Subscriber database or not"
          },
          "registeredKYC": {
            "type": "integer",
            "format": "int32",
            "description": "Total Registered KYC(s) with given ID",
            "default": 0
          },
          "activeKYC": {
            "type": "integer",
            "format": "int32",
            "description": "Total Active KYC(s) with given ID",
            "default": 0
          },
          "allowedKYC": {
            "type": "integer",
            "format": "int32",
            "description": "Remaining Allowed KYC(s) for given ID",
            "default": 0
          }
        },
        "required": [
          "type",
          "id",
          "status",
          "allowedKYC"
        ]
      },
      "QueryDealerResponse": {
        "title": "QueryDealerResponse",
        "type": "object",
        "properties": {
          "result": {
            "$ref": "#/components/schemas/Result"
          },
          "dealerInfo": {
            "$ref": "#/components/schemas/DealerInfo"
          }
        },
        "required": [
          "result"
        ],
        "description": "Query Dealer API Response"
      },
      "NIDStatusQueryResponse": {
        "title": "NID Status Query Response",
        "type": "object",
        "properties": {
          "result": {
            "$ref": "#/components/schemas/Result"
          },
          "nidInfo": {
            "$ref": "#/components/schemas/NIDInfo"
          }
        },
        "description": "NID Status Query API Response"
      },
      "IDStatusQueryResponse": {
        "title": "ID Status Query Response",
        "type": "object",
        "description": "ID Status Query API Response",
        "properties": {
          "result": {
            "$ref": "#/components/schemas/Result"
          },
          "nidInfo": {
            "$ref": "#/components/schemas/IDInfo"
          }
        }
      },
      "KYCStatusResponse": {
        "title": "KycStatusResponse",
        "type": "object",
        "description": "KYC Status Query Response",
        "properties": {
          "result": {
            "$ref": "#/components/schemas/Result"
          },
          "kycInfo": {
            "$ref": "#/components/schemas/KYCStatus"
          }
        },
        "required": [
          "result"
        ]
      },
      "KYCStatus": {
        "title": "KYCStatus",
        "type": "object",
        "properties": {
          "serviceNumber": {
            "type": "string",
            "pattern": "^([\\+]?)([0-9]){6,14}",
            "minLength": 6,
            "maxLength": 20,
            "description": "Service Number for which KYC Status was enquired"
          },
          "kycStatus": {
            "type": "string",
            "description": "Current Status of the KYC approval",
            "maxLength": 20,
